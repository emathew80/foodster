<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Edit Your Place</h1>
  <%= simple_form_for @place do |f| %>
    <div class="new_color">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    </div>
    <%= f.submit 'Update Foodster', class: 'btn btn-primary' %>
  <% end %>
</div>
